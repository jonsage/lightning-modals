<aura:component >
    <aura:attribute name="class" type="String"/>
    <aura:attribute name="component" type="String"/>
    <aura:attribute name="params" type="Map"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
	<div class="{!v.class}">
    	{!v.body}
    </div>
</aura:component>